<?xml version="1.0" encoding="UTF-8"?>
<customUI onLoad="Ribbon_Load" xmlns="http://schemas.microsoft.com/office/2006/01/customui">
    <ribbon>
        <tabs>
            <tab idMso="TabNewMailMessage">
                <group id="group1" insertBeforeMso="GroupClipboard" label="Distribution List">
                    <dropDown id="cbLists" getItemCount="OnGetItemCount" getItemLabel="OnGetItemLabel" onAction="cbLists_TextChanged" label="List" showImage="false" />
                    <button id="butManage" onAction="butManage_Click" label="Manage Lists" showImage="false" />
                </group>
            </tab>
        </tabs>
    </ribbon>
</customUI>